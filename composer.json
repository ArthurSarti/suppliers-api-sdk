{
    "name": "hitmeister/suppliers-api-sdk",
    "version": "0.0.1",
    "description": "Suppliers API SDK",
    "keywords": [
        "real.digital",
        "hitmeister",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://sms.real.de/api/docs",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "real.digital",
            "email": "sms.tech@real-digital.de"
        }
    ],
    "support": {
        "email": "sms.tech@real-digital.de",
        "source": "https://github.com/hitmeister/suppliers-api-sdk",
        "issues": "https://github.com/hitmeister/suppliers-api-sdk/issues"
    },
    "require": {
        "php": ">=5.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "satooshi/php-coveralls": "~1.0",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "SMS\\Suppliers\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "SMS\\Suppliers\\" : "test/" }
    }
}
